{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/utils/Signer.jx", "src/utils/Pinata.js", "src/utils/Web3Provider.js", "src/pages/My_Collection/Create_NFT/index.tsx", "src/pages/My_Collection/components/MyNFTList.js", "src/pages/My_Collection/index.js", "src/component/Navbar.js"],
  "exclude": ["node_modules"]
}
